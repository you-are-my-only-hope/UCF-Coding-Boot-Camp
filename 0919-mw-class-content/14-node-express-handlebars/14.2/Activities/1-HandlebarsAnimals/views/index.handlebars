<!-- 5. Output the animalType string, pet boolean and fierceness number to the page for all the animals. -->
<ul>
  {{#each anims}}
    <li>
      <p>Type: {{animalType}}</p>
      <p>Pet: {{pet}}</p>
      <p>Fierceness: {{fierceness}}</p>
    </li>
  {{/each}}
</ul>
<div id="nonPets"></div>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js" language="javascript"></script>
<script>
	jQuery(function($) {
		$.get('https://www.reddit.com/.json', function  (response){
//			console.log('ajax data', response);
			var html;
			$.each(response.data.children, function  (k, post){
				console.log('key ' + k, post);
				html += '<div>';
				html += '<h3>'+post.data.title+'</h3>';
				html += '<img src="' + post.data.thumbnail + '" />';
				html += '<a href="'+post.data.url+'">Keep reading...</a>'
				html += '</div>';
			});
			$(html).appendTo('#nonPets');
		});

		function adder (add1, add2, callback) {
		    var sum = add1 + add2;
		    callback(sum);
		}
		adder(1, 2, function  (sum){
			console.log('sum', sum);
		})
	});
</script>